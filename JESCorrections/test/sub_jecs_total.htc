executable            = fitJESCs_v3
getenv                = True
should_transfer_files = YES
when_to_transfer_output = ON_EXIT_OR_EVICT
transfer_output_files = jescs_total

arguments             =  -o jescs_total -n -1 -b -j ak4caloHLT
output                = htc_out/ak4caloHLT_total.out
error                 = htc_out/ak4caloHLT_total.err
log                   = htc_out/ak4caloHLT_total.log
+JobFlavour           = "testmatch"
queue

